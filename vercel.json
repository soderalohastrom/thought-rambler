{
  "buildCommand": "cd thought-ramble-parser && pnpm install && pnpm build",
  "outputDirectory": "thought-ramble-parser/dist",
  "framework": "vite",
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/parse-thoughts",
      "dest": "/api/parse-thoughts.py"
    },
    {
      "src": "/health", 
      "dest": "/api/health.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}