{
  "version": 2,
  "buildCommand": "cd thought-ramble-parser && pnpm install && pnpm build",
  "outputDirectory": "thought-ramble-parser/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/app.py"
    },
    {
      "source": "/health",
      "destination": "/backend/app.py"
    }
  ],
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  }
}