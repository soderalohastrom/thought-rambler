{
  "buildCommand": "cd thought-ramble-parser && pnpm install && pnpm build",
  "outputDirectory": "thought-ramble-parser/dist",
  "framework": "vite",
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/health", 
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}